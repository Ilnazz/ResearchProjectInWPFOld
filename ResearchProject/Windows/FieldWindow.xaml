<Window x:Class="ResearchProject.Windows.FieldWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:skiasharp="clr-namespace:SkiaSharp.Views.Desktop;assembly=SkiaSharp.Views.WindowsForms"
        xmlns:local="clr-namespace:ResearchProject"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized"
        WindowStyle="None">
    <Grid>
        <WindowsFormsHost>
            <skiasharp:SKGLControl x:Name="SKGLControl"
                                   VSync="True"/>
        </WindowsFormsHost>
    </Grid>
</Window>