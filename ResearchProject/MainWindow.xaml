<Window x:Class="ResearchProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:skiasharp="clr-namespace:SkiaSharp.Views.Desktop;assembly=SkiaSharp.Views.WindowsForms"
        xmlns:local="clr-namespace:ResearchProject"
        mc:Ignorable="d"
        Title="Эволюция"
        WindowStartupLocation="CenterScreen"
        MouseMove="Window_MouseMove"
        KeyDown="Window_KeyDown">
    <Grid>
        <WindowsFormsHost Grid.Row="1">
            <skiasharp:SKGLControl x:Name="SKGLControl"
                                    VSync="True"
                                    PaintSurface="FieldRenderer"/>
        </WindowsFormsHost>
    </Grid>
</Window>
